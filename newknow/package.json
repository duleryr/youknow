{
  "name": "ionic-hello-world",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "scripts": {
    "dev": "bnr dev",
    "build": "bnr build",
    "build-dev": "bnr build-dev",
    "test": "bnr test",
    "test:watch": "bnr test-watch",
    "e2e": "node config/protractor.server.js",
    "e2e:interactive": "node config/protractor.server.js --elementExplorer",
    "ionic:serve": "ionic-app-scripts serve",
    "ionic:build": "ionic-app-scripts build",
    "ios:dev": "bnr ios:dev",
    "ios:release": "bnr ios:release",
    "ios:release:ci": "bnr ios:release:ci",
    "android:dev": "bnr android:dev",
    "android:release": "bnr android:release",
    "cz": "git-cz",
    "docs": "typedoc --options typedoc.json --exclude '**/*+(e2e|spec|index).ts' ./src/",
    "release": "standard-version --no-verify",
    "push": "git push --follow-tags origin master",
    "lint": "tslint \"src/**/*.ts\" --exclude=src/**/*.d.ts",
    "scss-lint": "scss-lint",
    "outdated": "npm outdated --depth 0",
    "post-install": "webdriver-manager update",
    "precommit": "npm run lint && npm run scss-lint",
    "prepush": "npm test",
    "commitmsg": "validate-commit-msg"
  },
  "betterScripts": {
    "dev": {
      "command": "ionic serve",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "web"
      }
    },
    "build": {
      "command": "ionic-app-scripts build --prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build-dev": {
      "command": "ionic-app-scripts build --dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "test": {
      "command": "karma start",
      "env": {
        "NODE_ENV": "test"
      }
    },
    "test-watch": {
      "command": "karma start --auto-watch --no-single-run",
      "env": {
        "NODE_ENV": "test",
        "NO_COVERAGE": true
      }
    },
    "ios:dev": {
      "command": "ionic run ios --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "ios"
      }
    },
    "ios:release": {
      "command": "ionic build ios --prod",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "ios:release:ci": {
      "command": "npm run build",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "ios"
      }
    },
    "android:dev": {
      "command": "ionic run android --livereload",
      "env": {
        "NODE_ENV": "development",
        "PLATFORM": "android"
      }
    },
    "android:release": {
      "command": "ionic build android --prod",
      "env": {
        "NODE_ENV": "production",
        "PLATFORM": "android"
      }
    }
  },
  "dependencies": {
    "@angular/common": "2.4.8",
    "@angular/compiler": "2.4.8",
    "@angular/compiler-cli": "2.4.8",
    "@angular/core": "2.4.8",
    "@angular/forms": "2.4.8",
    "@angular/http": "2.4.8",
    "@angular/platform-browser": "2.4.8",
    "@angular/platform-browser-dynamic": "2.4.8",
    "@angular/platform-server": "2.4.8",
    "@ionic-native/file": "^3.4.2",
    "@ionic/storage": "2.0.1",
    "ionic-angular": "2.3.0",
    "ionic-native": "^2.4.1",
    "ionicons": "3.0.0",
    "rxjs": "5.0.1",
    "sw-toolbox": "3.4.0",
    "zone.js": "0.7.2"
  },
  "devDependencies": {
    "@ionic/app-scripts": "1.1.4",
    "@types/jasmine": "ts2.0",
    "@types/lodash-es": "ts2.0",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.1.2",
    "better-npm-run": "0.0.15",
    "codelyzer": "3.0.0-beta.0",
    "commitizen": "^2.9.6",
    "css-loader": "^0.27.3",
    "cz-conventional-changelog": "^2.0.0",
    "finalhandler": "^1.0.1",
    "husky": "^0.13.3",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "^2.5.2",
    "jasmine-spec-reporter": "^3.2.0",
    "json-loader": "^0.5.4",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-jasmine": "^1.1.0",
    "karma-mocha-reporter": "^2.2.3",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-remap-coverage": "0.1.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "2.0.2",
    "phantomjs-prebuilt": "^2.1.14",
    "protractor": "4.0.14",
    "protractor-jasmine2-screenshot-reporter": "^0.3.3",
    "raw-loader": "0.5.1",
    "serve-static": "^1.12.1",
    "source-map-loader": "^0.1.6",
    "standard-version": "^4.0.0",
    "to-string-loader": "^1.1.4",
    "ts-helpers": "^1.1.1",
    "ts-node": "^3.0.2",
    "tslint": "4.5.1",
    "tslint-loader": "^3.4.3",
    "typedoc": "^0.5.9",
    "typescript": "~2.0.10",
    "validate-commit-msg": "^2.11.2",
    "webpack": "2.2.1",
    "xml2js": "^0.4.17",
    "@types/node": "^7.0.12",
    "codecov": "^2.1.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-remap-istanbul": "^0.6.0",
    "tslint-eslint-rules": "^4.0.0"
  },
  "cordovaPlugins": [
    "cordova-plugin-whitelist",
    "cordova-plugin-statusbar",
    "cordova-plugin-console",
    "cordova-plugin-device",
    "cordova-plugin-splashscreen",
    "ionic-plugin-keyboard"
  ],
  "cordovaPlatforms": [],
  "description": "ionic2-magic-ball: An Ionic project"
}